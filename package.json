    "react",
    "typescript",
    "material-design",
    "docker"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "live-server": "^1.2.2",
    "typescript": "^5.5.3"
  }
}